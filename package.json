{
  "name": "bing-create",
  "version": "0.0.1",
  "description": "Generate images using Bing Image Creator",
  "keywords": [
    "bing",
    "ai",
    "text-to-image",
    "image-generation",
    "chatgpt"
  ],
  "license": "Apache-2.0",
  "author": "David Duran <dadch1404@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/totallynotdavid/bing-create.git"
  },
  "bugs": {
    "url": "https://github.com/totallynotdavid/bing-create/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "readme.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "check": "biome check . && bun x prettier \"**/*.{md,yml}\" --write --print-width=80 --prose-wrap=always",
    "build": "bun run check && bun x bunup",
    "test": "bun test",
    "prepublishOnly": "bun run clean && bun run build"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "shx": "^0.4.0"
  }
}
